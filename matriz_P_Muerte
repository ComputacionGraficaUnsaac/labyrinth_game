	for i in range(len(matrix)):
		for j in range(len(matrix[0])):
			if matrix[i][j] == 1:
				set_pixel(y - j, x - i, r, g, b, size)
      
			#Color Oz
			if matrix[i][j] == 2:
				set_pixel(y - j, x - i,255/255, 0/255, 0/255, size)
			#Color tuncia
			if matrix[i][j] == 3:
				set_pixel(y - j, x - i,163/255, 73/255, 164/255, size)
